<?xml version="1.0" encoding="UTF-8"?>

<LD.Widgets.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_height="match_parent">

        <LD.Widgets.InertCheckBox
            android:id="@+id/itemCheckBox"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="5dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:button="@drawable/checkbox"
            android:focusable="false" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_weight="1"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textViewfullname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Full name : "
                android:textColor="@android:color/white"
                android:textSize="20sp"
                android:textAppearance="?android:attr/textAppearanceMedium" />

            <TextView
                android:id="@+id/dl_fullnameTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textViewbussname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Business name : "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_bussnameTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textViewstndfullname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Stnd Full name : "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_stndfullnameTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView1a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Service Type: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_servicetypeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/linearLayout1a"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#294973"
                android:orientation="vertical" />

            <TextView
                android:id="@+id/textViewCasepaperType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Case Paper Type: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_CasepaperTypeTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="DuplicateIds">

            <TextView
                android:id="@+id/textView2a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Address: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_addressTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/linearLayout2c"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#294973"
                android:orientation="vertical" />

            <TextView
                android:id="@+id/textView2c"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="City: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_cityTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

        </LinearLayout>



        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView2b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Apt: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_aptTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/linearLayout2b"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#294973"
                android:orientation="vertical" />

            <TextView
                android:id="@+id/textView2d"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="State: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_stateTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

            <LinearLayout
                android:id="@+id/linearLayout2d"
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#294973"
                android:orientation="vertical" />

            <TextView
                android:id="@+id/textView2e"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Zip: "
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/white"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/dl_zipTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="- Not set -"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@android:color/holo_blue_dark"
                android:textSize="18sp" />

        </LinearLayout>


    </LinearLayout>


</LD.Widgets.CheckableLinearLayout>