<?xml version="1.0" encoding="utf-8"?>
<LD.Widgets.CheckableLinearLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

      	<LD.Widgets.InertCheckBox android:id="@+id/itemCheckBox"
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginRight="5dp"
			android:layout_alignParentRight="true" 
			android:layout_centerVertical="true" 
			android:focusable="false" 
			android:button="@drawable/checkbox" />
        	
       	<TextView
            android:id="@+id/textView1a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Service Type : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_servicetypeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
       	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout1a" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
        
       	<TextView
            android:id="@+id/textView1b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="LT Service : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_ltservicetypeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
       	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout1b" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
       	       	
       	<TextView
            android:id="@+id/textView1c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Full name : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_fullnameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView2a"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Address : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_addressTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
     	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout2a" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
     	
    	<TextView
            android:id="@+id/textView2b"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Apt : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_aptTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
     	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout2b" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
     	
        <TextView
            android:id="@+id/textView2c"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="City : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_cityTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
     	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout2c" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
     	
        <TextView
            android:id="@+id/textView2d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="State : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_stateTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />
        
     	<LinearLayout 
			android:layout_marginLeft="2dp" 
			android:layout_marginRight="2dp" 
			android:layout_height="match_parent" 
			android:id="@+id/linearLayout2d" 
			android:layout_width="1dp" 
			android:orientation="vertical" 
			android:background="#294973" />
     	
        <TextView
            android:id="@+id/textView2e"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Zip : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/dl_zipTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="- Not set -"
            android:textAppearance="?android:attr/textAppearanceMedium" />			

    </LinearLayout>
    
  	<LinearLayout 
		android:layout_marginTop="2dp" 
		android:layout_marginBottom="2dp" 
		android:layout_height="1dp" 
		android:id="@+id/linearLayout1" 
		android:layout_width="match_parent" 
		android:orientation="horizontal" 
		android:background="#294973" />

</LD.Widgets.CheckableLinearLayout>